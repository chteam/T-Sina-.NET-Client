<Window x:Class="TSinaApi.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="332" Width="325" xmlns:my="clr-namespace:TSinaApi.Client">
	<Window.Resources>
		<Storyboard x:Key="Fade">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Name="FadeTarget"  Storyboard.TargetName="loginBox" 
                                           Storyboard.TargetProperty="(FrameworkElement.Height)">
                <SplineDoubleKeyFrame KeyTime="00:00:00"  Value="50" x:Name="FadeTargetFrom"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0" x:Name="FadeTargetTo"/>
			</DoubleAnimationUsingKeyFrames>
		</Storyboard>
		
	</Window.Resources>

    <StackPanel Name="ContentPanel">
       
        <!--<my:SendBox HorizontalAlignment="Left"  x:Name="sendBox" VerticalAlignment="Top" />
        <StackPanel Height="211" HorizontalAlignment="Left" Name="stackPanel1" VerticalAlignment="Top" Width="229"></StackPanel>-->

    </StackPanel>
</Window>
